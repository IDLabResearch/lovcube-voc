#
# Generic description of Restriction Types
#

lrd:FunctionalPropertiesDefinition a rdfcv:SimpleConstraint .
lrd:InverseFunctionalPropertiesDefinition a rdfcv:SimpleConstraint .
lrd:SubsumptionDefinition a rdfcv:SimpleConstraint .
lrd:PropertyDomainsDefinition a rdfcv:SimpleConstraint .
lrd:PropertyRangesDefinition a rdfcv:SimpleConstraint .
lrd:LiteralPatternMatchingDefinition a rdfcv:SimpleConstraint .
lrd:UniversalQuantificationDefinition a rdfcv:SimpleConstraint .
lrd:MinimumUnqualifiedCardinalityDefinition a rdfcv:SimpleConstraint .
lrd:MinimumQualifiedCardinalityDefinition a rdfcv:SimpleConstraint .
lrd:MaximumUnqualifiedCardinalityDefinition a rdfcv:SimpleConstraint .
lrd:MaximumQualifiedCardinalityDefinition a rdfcv:SimpleConstraint .
lrd:AsymmetricPropertiesDefinition a rdfcv:SimpleConstraint .
lrd:IrreflexivePropertiesDefinition a rdfcv:SimpleConstraint .
lrd:DisjointPropertiesDefinition a rdfcv:SimpleConstraint .
lrd:DisjointClassesDefinition a rdfcv:SimpleConstraint .

