#
# Generic description of Restriction Types
#

lovstats:FunctionalPropertiesDefinition a rdfcv:SimpleConstraint .
lovstats:InverseFunctionalPropertiesDefinition a rdfcv:SimpleConstraint .
lovstats:SubsumptionDefinition a rdfcv:SimpleConstraint .
lovstats:PropertyDomainsDefinition a rdfcv:SimpleConstraint .
lovstats:PropertyRangesDefinition a rdfcv:SimpleConstraint .
lovstats:LiteralPatternMatchingDefinition a rdfcv:SimpleConstraint .
lovstats:UniversalQuantificationDefinition a rdfcv:SimpleConstraint .
lovstats:MinimumUnqualifiedCardinalityDefinition a rdfcv:SimpleConstraint .
lovstats:MinimumQualifiedCardinalityDefinition a rdfcv:SimpleConstraint .
lovstats:MaximumUnqualifiedCardinalityDefinition a rdfcv:SimpleConstraint .
lovstats:MaximumQualifiedCardinalityDefinition a rdfcv:SimpleConstraint .
lovstats:AsymmetricPropertiesDefinition a rdfcv:SimpleConstraint .
lovstats:IrreflexivePropertiesDefinition a rdfcv:SimpleConstraint .
lovstats:DisjointPropertiesDefinition a rdfcv:SimpleConstraint .
lovstats:DisjointClassesDefinition a rdfcv:SimpleConstraint .

#
