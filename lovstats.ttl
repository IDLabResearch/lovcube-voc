    lovc:hasRestrictionTypeDefinition lovstats:FunctionalPropertiesDefinition ;
    rdfs:isDefinedBy <https://publikationen.bibliothek.kit.edu/1000054062> ;
    rdfs:comment """"""@en ;
    rdfs:label "Functional properties restriction type"@en .

